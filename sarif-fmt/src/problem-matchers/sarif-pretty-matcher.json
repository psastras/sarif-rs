{
  "problemMatcher": [
    {
      "owner": "sarif-pretty",
      "pattern": [
        {
          "regexp": "^(note|info|warning|error):\\s(.+)$",
          "severity": 1,
          "message": 2
        },
        {
          "regexp": "^\\s*[/\\W+/g]*\\s*(.*):([0-9]+):([0-9]+)$",
          "file": 1,
          "line": 2,
          "column": 3
        }
      ]
    }
  ]
}
